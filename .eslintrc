{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "env": {
        "browser": true,
        "es6": false
    },
    "rules": {
        "indent": ["error", 4],
        "prefer-arrow-callback": "off",
        "func-names": "off",
        "no-var": "off",
        "comma-dangle": "off",
        "object-shorthand": "off",
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    },
    "globals": {
        "$": true,
        "scrollIt": true,
        "isElementInViewport": true
    }
}
